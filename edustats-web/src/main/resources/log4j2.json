{
  "configuration": {
    "status": "error",
    "name": "RoutingTest",
    "packages": "org.apache.logging.log4j.test",
    "properties": {
      "property": {
        "name": "filename",
        "value": "target/rolling1/rollingtest-$${sd:type}.log"
      }
    },
    "ThresholdFilter": {
      "level": "all"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%m%n"
        }
      }
    },
    "loggers": {
      "logger": {
        "name": "EventLogger",
        "level": "error",
        "additivity": "false",
        "AppenderRef": {
          "ref": "Routing"
        }
      },
      "root": {
        "level": "error",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}